{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "ES2020",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@mui/material": ["node_modules/@mui/material"]
    },

    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
  },
  "include": ["src"]
}